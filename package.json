{
    "name": "mobileframeworksystem",
    "version": "1.0.0",
    "private": true,
    "devDependencies": {
        "@types/jest": "^23.3.0",
        "@types/query-string": "^6.2.0",
        "@types/react": "^16.8.6",
        "@types/react-native": "^0.60.24",
        "@types/react-native-popup-dialog": "^0.16.2",
        "@types/react-native-vector-icons": "^4.3.1",
        "@types/react-navigation": "^1.0.19",
        "babel-eslint": "8.1.0",
        "babel-jest": "^23.4.0",
        "babel-preset-react-native": "^3.0.1",
        "concurrently": "3.5.1",
        "eslint": "4.14.0",
        "husky": "0.14.3",
        "jest-expo": "^36.0.0",
        "react-test-renderer": "^16.3.1",
        "rimraf": "^2.6.2",
        "ts-jest": "24.0.0",
        "tslint": "^5.8.0",
        "typescript": "^3.1.6"
    },
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "npm run build && concurrently -r 'npm run watch' 'expo start'",
        "eject": "expo eject",
        "test": "yarn jest && tslint src/**/*.ts && eslint .",
        "watch": "npm run build -- -w",
        "ios": "npm run build && concurrently -r 'npm run watch' 'expo start --ios'",
        "android": "npm run build && concurrently -r 'npm run watch' 'expo start --android'",
        "tsc": "tsc",
        "clean": "rimraf artifacts",
        "build": "npm run clean && npm run tsc --",
        "lint": "tslint src/**/*.ts",
        "jest": "node node_modules/jest/bin/jest.js"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
            "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "transformIgnorePatterns": [
            "node_modules/(?!(react-native|lottie-react-native|expo|react-native-maps|react-native-svg|react-native-branch|native-base-shoutem-theme|react-native-easy-grid|react-native-drawer|react-native-vector-icons|react-native-keyboard-aware-scroll-view|react-navigation|native-base|@expo|react-native-scrollable-tab-view)/)"
        ]
    },
    "dependencies": {
        "@babel/template": "^7.2.2",
        "@babel/traverse": "^7.3.4",
        "@babel/types": "^7.3.4",
        "@expo/vector-icons": "^10.0.6",
        "@react-native-community/netinfo": "~3.2.1",
        "@types/node": "^11.9.6",
        "axios": "^0.19.0",
        "babel-plugin-flow-react-proptypes": "^25.0.0",
        "babel-preset-expo": "^7.1.0",
        "expo": "^35.0.0",
        "expo-barcode-scanner": "~7.0.0",
        "expo-camera": "~7.0.0",
        "expo-constants": "~7.0.0",
        "expo-font": "~7.0.0",
        "expo-location": "~7.0.0",
        "expo-permissions": "~7.0.0",
        "lodash": "4.17.15",
        "loose-envify": "^1.4.0",
        "native-base": "^2.12.1",
        "object-assign": "^4.1.1",
        "prop-types": "^15.7.2",
        "query-string": "^6.1.0",
        "react": "16.3.1",
        "react-is": "^16.9.0",
        "react-native": "0.59.8",
        "react-native-awesome-alerts": "^1.2.0",
        "react-native-device-info": "^0.24.3",
        "react-native-gesture-password": "^0.3.6",
        "react-native-popup-dialog": "^0.17.0",
        "react-native-table-component": "^1.1.8",
        "react-native-tableview": "^2.4.0",
        "react-native-tableview-simple": "^0.18.1",
        "react-native-uuid-generator": "^4.0.1",
        "react-navigation": "1.6.1",
        "react-redux": "5.0.6",
        "redux": "3.7.2",
        "redux-form": "7.2.0",
        "redux-persist": "5.4.0",
        "redux-thunk": "2.2.0",
        "remote-redux-devtools": "0.5.0",
        "remote-redux-devtools-on-debugger": "^0.8.0"
    }
}